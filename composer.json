{
  "name": "phanda-framework/framework",
  "description": "Phanda is a fast, modern PHP framework.",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "framework",
    "phanda"
  ],
  "authors": [
    {
      "name": "Storm Burpee",
      "email": "storm@phandaframework.com"
    }
  ],
  "require": {
    "php": "^7.1.3",
    "ext-json": "*",
    "filp/whoops": "^2.3",
    "phanda-framework/dotenv": "^0.1.2",
    "symfony/console": "^4.2",
    "symfony/dependency-injection": "^4.2",
    "symfony/event-dispatcher": "^4.2",
    "symfony/filesystem": "^4.2",
    "symfony/finder": "^4.2",
    "symfony/http-foundation": "^4.2",
    "symfony/http-kernel": "^4.2",
    "symfony/process": "^4.2",
    "symfony/routing": "^4.2",
    "symfony/var-dumper": "^4.2",
    "ext-pdo": "*"
  },
  "autoload": {
    "files": [
      "src/Phanda/Foundation/helpers.php",
      "src/Phanda/Support/helpers.php"
    ],
    "psr-4": {
      "Phanda\\": "src/Phanda/"
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
