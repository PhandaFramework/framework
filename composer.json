{
    "name": "phanda/framework",
    "description": "Phanda is a fast, modern PHP framework.",
    "type": "library",
    "license": "MIT",
    "keywords": ["framework", "phanda"],
    "authors": [
        {
            "name": "Storm Burpee",
            "email": "storm@solarreviews.com"
        }
    ],
    "require": {
        "php": "^7.1.3"
    },
    "autoload": {
        "files": [
            "src/Phanda/Support/helpers.php"
        ],
        "psr-4": {
            "Phanda\\": "src/Phanda/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
